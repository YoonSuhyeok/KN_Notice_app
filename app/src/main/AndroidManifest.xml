<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.noticekangwon">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_main"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_main_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".Activity.SplashActivity"
                  android:configChanges="uiMode">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activity.MainActivity">
        </activity>

        <activity android:name=".Activity.DevelopInfoActivity" android:parentActivityName=".Activity.MainActivity"></activity>

        <activity android:name=".Activity.BookMarkActivity" android:parentActivityName=".Activity.MainActivity"></activity>

        <activity android:name=".Activity.FilterActivity"></activity>

<!--        <service android:name=".notifiThread.MyService"/>-->
    </application>
    <uses-permission android:name="android.permission.INTERNET" />

</manifest>